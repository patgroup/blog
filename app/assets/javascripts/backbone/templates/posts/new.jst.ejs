
<form id="new-post" name="post">
	<fieldset>
		<legend>Edit post</legend>
		<label>Title:</label>
		<input class="required" type="text" name="title" id="title" value="<%= title %>" >
		<label>Content:</label>
		<textarea class="required" rows="3" name="content" id="content"><%= content %></textarea>
		<br/>
		<button type="submit" class="btn">Create Post</button>
	</fieldset>
	
